model UserPhotoCard {
  id          String   @id @default(uuid())
  ownerId     String
  photoCardId String
  quantity    Int
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt

  saleCard SaleCard[]

  @@unique([ownerId, photoCardId]) // ownerId_photoCardId
  @@index([ownerId])
}
