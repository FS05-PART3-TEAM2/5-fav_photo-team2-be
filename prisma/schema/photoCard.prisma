model PhotoCard {
  id          String   @id @default(uuid())
  name        String
  genre       String
  grade       String
  price       Int
  description String
  imageUrl    String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt

  saleCard  SaleCard[]
  creator   User       @relation(fields: [creatorId], references: [id])
  creatorId String

  @@index([grade, genre])
}
